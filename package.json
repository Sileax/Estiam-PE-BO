{
  "name": "universal-react-tutorial",
  "version": "0.0.0",
  "description": "A tutorial on building a universal React web application.",
  "author": "Mark <mark@remarkablemark.org>",
  "scripts": {
    "start": "webpack && node server.js",
    "dev": "npm-run-all --parallel watch:*",
    "watch:webpack": "webpack -w",
    "watch:server": "nodemon --ext js,jsx --ignore public/ server.js"
  },
  "main": "server.js",
  "keywords": [
    "universal",
    "isomorphic",
    "react",
    "tutorial"
  ],
  "dependencies": {
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.9.0",
    "cookie-parser": "^1.4.3",
    "express": "^4.13.4",
    "fetch-everywhere": "^1.0.5",
    "react": "^15.0.2",
    "react-cookie": "^1.0.4",
    "react-dom": "^15.0.2",
    "react-modal": "^1.6.5",
    "react-modal-dialog": "^4.0.5",
    "react-notification": "^6.6.0",
    "react-redux": "^4.4.6",
    "react-router": "^2.8.1",
    "redux": "^3.6.0",
    "webpack": "^1.13.1",
    "whatwg-fetch": "^2.0.2"
  },
  "devDependencies": {
    "nodemon": "^1.11.0",
    "npm-run-all": "^3.1.2"
  },
  "private": true,
  "license": "MIT"
}
